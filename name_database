/**
 * View WooCommerce subscription from all orders list
 */
function wpsc_view_woo_subscription_tbl(ticket_id, order_id) {

	wpsc_close_modal();
	setTimeout(
		function () {
			wpsc_view_woo_subscription( ticket_id, order_id );
		},
		500
	);
}

